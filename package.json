{
  "name": "sveltekit-tw-demo",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --open",
    "build": "rm -fdr ./build && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview --open",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "description": "SvelteKit demo app with Typescript, Prettier, ESLint, Tailwind",
  "main": "index.js",
  "author": "Glen Ihrig",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "4.5.8",
    "@lukeed/uuid": "2.0.0",
    "cookie": "0.5.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "1.0.0-next.78",
    "@sveltejs/kit": "1.0.0-next.350",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.2",
    "@types/cookie": "0.5.1",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "autoprefixer": "10.4.7",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "postcss": "8.4.14",
    "postcss-load-config": "4.0.1",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.11",
    "svelte": "3.48.0",
    "svelte-check": "2.7.2",
    "svelte-preprocess": "4.10.7",
    "tailwindcss": "3.1.1",
    "tslib": "2.4.0",
    "typescript": "4.7.3"
  }
}
